datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Farm {
  id        Int     @id @default(autoincrement())
  farm_id   Int
  name      String
  longitude Float
  latitude  Float
  location  String
}

model SensorData {
  id      Int     @id @default(autoincrement())
  //type    DataType
  value   Float
  farmId  Int
}

