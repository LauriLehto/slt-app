datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Farm {
  id        Int     @id @default(autoincrement())
  title     String
  longitude Float
  latitude  Float
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model SensorData {
  id      Int     @id @default(autoincrement())
  type    DataType
  value   Float
  farmId  Int
}

enum DataType {
  RAIN
  TEMPERATURE
}